{
    "source": {

      "include": ["./screens/userScreens"],
      "includePattern": ".js",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/component",
        "node_modules/better-docs/category"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/better-docs"
    },
    "templates": {
        "better-docs": {
            "name": "Client AIDC"
        }
    }
  }